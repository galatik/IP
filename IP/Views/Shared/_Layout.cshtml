<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - IP</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css"/>
    <link rel="stylesheet" href="~/css/site.css"/>
       @* Uncomment to use the HtmlEditor control *@
        @* <script src="https://cdnjs.cloudflare.com/ajax/libs/quill/1.3.6/quill.min.js"></script> *@
    
        @* Uncomment to use the Gantt control *@
        @*<link href="~/css/devextreme/dx-gantt.css" rel="stylesheet" />*@
    
        @* Uncomment to use the Diagram control *@
        @*<link href="~/css/devextreme/dx-diagram.css" rel="stylesheet" />*@
    
        <link href="~/css/devextreme/bootstrap.css" rel="stylesheet"/>
    
        <link href="~/css/devextreme/dx.common.css" rel="stylesheet"/>
        @* Uncomment to use the Gantt control *@
        @*<script src="~/lib/devextreme/dx-gantt.js"></script>*@
    
    
        @* Uncomment to use the Diagram control *@
        @*<script src="~/lib/devextreme/dx-diagram.js"></script>*@
    
        <script src="~/lib/devextreme/jquery.js"></script>
        <script src="~/lib/jquery.unobtrusive/jquery.unobtrusive-ajax.min.js"></script>
        <script src="~/lib/devextreme/bootstrap.js"></script>
    
    <link href="~/css/devextreme/dx.material.rn-scheme.css" rel="stylesheet"/>
    
    @* Uncomment to use Globalize for localization *@
        @* Docs: https://docs.devexpress.com/AspNetCore/400577#globalize *@
        @*<script src="~/lib/devextreme/cldr.js"></script>*@
        @*<script src="~/lib/devextreme/cldr/event.js"></script>*@
        @*<script src="~/lib/devextreme/cldr/supplemental.js"></script>*@
        @*<script src="~/lib/devextreme/cldr/unresolved.js"></script>*@
        @*<script src="~/lib/devextreme/globalize.js"></script>*@
        @*<script src="~/lib/devextreme/globalize/message.js"></script>*@
        @*<script src="~/lib/devextreme/globalize/number.js"></script>*@
        @*<script src="~/lib/devextreme/globalize/currency.js"></script>*@
        @*<script src="~/lib/devextreme/globalize/date.js"></script>*@
    
        @* Uncomment to enable client-side export *@
        @*<script src="~/lib/devextreme/jszip.js"></script>*@
    
        <script src="~/lib/devextreme/dx.all.js"></script>
    
        @* Uncomment to provide geo-data for the VectorMap control *@
        @* Docs: https://js.devexpress.com/DevExtreme/Guide/Widgets/VectorMap/Providing_Data/#Data_for_Areas *@
        @*<script src="~/js/devextreme/vectormap-data/world.js"></script>*@
    
        <script src="~/lib/devextreme/aspnet/dx.aspnet.mvc.js"></script>
        <script src="~/lib/devextreme/aspnet/dx.aspnet.data.js"></script>
        <script src="~/lib/devextreme/localization/dx.messages.ru.js"></script>
</head>
<body>
@await Html.PartialAsync("Layout/_Header")
<div class="container">
    <main role="main" id="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="border-top footer text-muted">
    <div class="container">
        &copy; 2021 - IP - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
    </div>
</footer>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</body>
</html>